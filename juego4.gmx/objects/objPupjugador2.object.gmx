if (place_meeting(x, y+1, objBloquepadre))
{ 
    jumps = jumpmax;
}

if (key_jump) &amp;&amp; (jumps &gt; 0)  
{ 
    jumps -= 1; 
    vsp = -jumpspeed;
}  
    
var hsp_final = hsp + hsp_carry;
hsp_carry = 0;

// horizontal collition 
if (place_meeting(x+hsp_final, y, objBloquepadre))
{
    while(!place_meeting(x+sign(hsp_final), y, objBloquepadre))
    {
        x += sign(hsp_final);
    }
    hsp_final = 0;
    hsp = 0;
}
x += hsp_final;


// vertical collition 

if (place_meeting(x, y+vsp, objBloquepadre))
{
    while(!place_meeting(x, y+sign(vsp), objBloquepadre))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;
